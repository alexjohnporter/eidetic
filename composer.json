{
  "name": "rawkode/eidetic",
  "description": "CQRS and EventSourcing package for php >= 5.4",
  "license": "MIT",
  "type": "library",
  "authors": [
      {
          "name": "David McKay",
          "email": "david@rawkode.com"
      }
  ],
  "keywords": [
    "CQRS",
    "EventSourcing"
  ],
  "autoload": {
    "psr-4": {
      "Rawkode\\Eidetic\\": "src//"
    }
  },
  "config": {
    "bin-dir": "bin",
    "vendor-dir": "var/vendor"
  },
  "require": {
    "php": ">=5.4",
    "fabpot/php-cs-fixer": "^1.10"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.1",
    "phpunit/phpunit": "~4.3"
  }
}
